<?xml version="1.0" encoding="UTF-8"?>
<dataflow xmlns="http://www.example.org/mml-schema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.example.org/mml-schema mml-schema.xsd ">

	<input name="cod" type="String"/>
	<input name="aaOff" type="String"/>

	<variable name="logs" type="java.util.Collection" >
			<body>new java.util.ArrayList();</body>
	</variable>

	<invokeservice outputVar="logs" serviceId="smartcampus.service.esse3"  methodName="GetAdLogs">
		<parameters>
			<entry name="cod">
			 	<value>cod</value>
			 </entry>
			<entry name="aaOff">
			 	<value>aaOff</value>
			 </entry>			 
		</parameters>
	</invokeservice>	


	<script outputVar="output">
		<className>smartcampus.service.esse3.script.Esse3WSScript</className>
		<method>getExamTeacher</method>
		<variables>
			<value>cod</value>
			<value>aaOff</value>
			<value>logs</value>
		</variables>
	</script>


</dataflow>
