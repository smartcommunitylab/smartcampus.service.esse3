<?xml version="1.0" encoding="UTF-8"?>
<dataflow xmlns="http://www.example.org/mml-schema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.example.org/mml-schema mml-schema.xsd ">

	<input name="cod" type="String"/>
	<input name="aaOff" type="String"/>

	<variable name="description" type="java.util.List&lt;Message&gt;" >
			<body>new java.util.ArrayList&lt;Message&gt;();</body>
	</variable>
	<variable name="teacher" type="java.util.List&lt;Message&gt;" >
			<body>new java.util.ArrayList&lt;Message&gt;();</body>
	</variable>

	<invokeservice outputVar="description" serviceId="smartcampus.service.esse3" methodName="GetExamDescription" writeMethod="addAll" writeAction="append">
		<parameters>
			<entry name="cod">
			 	<value>cod</value>
			 </entry>
			<entry name="aaOff">
			 	<value>aaOff</value>
			 </entry>			 
		</parameters>
	</invokeservice>	
	
	<invokeservice outputVar="teacher" serviceId="smartcampus.service.esse3" methodName="GetExamTeacher" writeMethod="addAll" writeAction="append">
		<parameters>
			<entry name="cod">
			 	<value>cod</value>
			 </entry>
			<entry name="aaOff">
			 	<value>aaOff</value>
			 </entry>			 
		</parameters>
	</invokeservice>		

	<script outputVar="output" >
		<className>smartcampus.service.esse3.script.Esse3WSScript</className>
		<method>mergeExamData</method>
		<variables>
			<value>cod</value>
			<value>aaOff</value>
			<value>description</value>
			<value>teacher</value>
		</variables>
	</script>


</dataflow>
