<?xml version="1.0" encoding="UTF-8"?>
<dataflow xmlns="http://www.example.org/mml-schema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.example.org/mml-schema mml-schema.xsd ">

	<!-- <input name="fiscalCode" type="String"/> <script outputVar="output"> <className>smartcampus.service.esse3.script.Esse3WSScript</className> <method>getStudent</method> <variables> <value>fiscalCode</value> </variables> </script> -->

	<input name="idAda" type="String" />
	
	<variable name="studentData" type="String" />

	<httpConnector name="adaConnector" method="GET" encoding="ISO-8859-1"/>

	<connect name="c1" connector="adaConnector" outputVar="studentData" transformer="string">
		<properties>
			<property name="url">
				<value>"http://localhost:8080/smartcampus.vas.portfolio.ada/rest/anagraficaestesa/" + idAda</value>
			</property>
		</properties>
	</connect>

	<script outputVar="output">
		<className>smartcampus.service.esse3.script.ADAConnectionScript</className>
		<method>getStudentData</method>
		<variables>
			<value>studentData</value>
			<value>idAda</value>
		</variables>
	</script>


</dataflow>
